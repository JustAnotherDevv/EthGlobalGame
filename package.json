{
  "name": "ethglobal-hackmoney",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev --workspace=@ethglobal-hackmoney/frontend",
    "dev:backend": "npm run dev --workspace=@ethglobal-hackmoney/backend",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=@ethglobal-hackmoney/frontend",
    "build:backend": "npm run build --workspace=@ethglobal-hackmoney/backend",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
